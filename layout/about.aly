{
  LinearLayout;
  backgroundColor="0xFFFFFFFF";
  layout_height="fill";
  gravity="center";
  layout_width="fill";
  {
    RelativeLayout;
    layout_width="match_parent";
    layout_height="match_parent";
    {
      ImageView;
      src="res/image/background1.png";
      layout_width="match_parent";
      scaleType="centerCrop";
      layout_height="match_parent";
    };
    {
      PullingLayout;
      id="imagePulling";
      PullDownEnabled=true;
      {
        ScrollView;
        layout_width="match_parent";
        layout_height="match_parent";
        {
          LinearLayout;
          orientation="vertical";
          layout_height="wrap_content";
          layout_width="match_parent";
          {
            LinearLayout;
            layout_width="match_parent";
            layout_height="20%h";
          };
          {
            RelativeLayout;
            layout_width="match_parent";
            {
              LinearLayout;
              layout_marginTop="50dp";
              layout_width="match_parent";
              {
                CardView;
                radius="40dp";
                Elevation="10";
                layout_width="match_parent";
                CardBackgroundColor="0xFFFFFFFF";
                layout_marginTop="5dp";
                layout_height="200dp";
              };
            };
            {
              LinearLayout;
              orientation="vertical";
              backgroundColor="0xFFFFFFFF";
              layout_marginTop="110dp";
              gravity="top|center";
              layout_width="match_parent";
              {
                TextView;
                text="初夏 Tools";
                textColor="0xFF000000";
                textSize="30";
                id="nameText";
              };
              {
                TextView;
                text="V1.0";
                textColor="0xFF000000";
                textSize="18";
                id="versionText";
              };
              {
                TextView;
                textSize="15";
                layout_marginTop="5dp";
                text=[==[「本项目已在GitHub开源.」
「这是一款由个人制作的综合运行时工具箱.」]==];
                textColor="0xFF595959";
                gravity="center";
                id="introductionText";
              };
              {
                LinearLayout;
                orientation="horizontal";
                layout_marginRight="30dp";
                layout_width="match_parent";
                layout_marginTop="5dp";
                layout_marginLeft="30dp";
                layout_height="75dp";
                {
                  LinearLayout;
                  gravity="center";
                  layout_height="match_parent";
                  {
                    LinearLayout;
                    orientation="vertical";
                    layout_height="match_parent";
                    gravity="center";
                    id="chatButton";
                    {
                      ImageView;
                      layout_marginRight="10dp";
                      layout_width="45dp";
                      src="res/image/icons8-chat.png";
                      layout_marginLeft="10dp";
                      layout_height="45dp";
                    };
                    {
                      TextView;
                      textSize="13";
                      layout_marginTop="5dp";
                      textColor="0xFF000000";
                      text="交流群聊";
                      id="chatButtonText";
                    };
                  };
                };
                {
                  LinearLayout;
                  layout_weight="1";
                  layout_height="match_parent";
                };
                {
                  LinearLayout;
                  gravity="center";
                  layout_height="match_parent";
                  {
                    LinearLayout;
                    orientation="vertical";
                    layout_height="match_parent";
                    gravity="center";
                    id="shareButton";
                    {
                      ImageView;
                      layout_marginRight="10dp";
                      layout_width="45dp";
                      src="res/image/icons8-share.png";
                      layout_marginLeft="10dp";
                      layout_height="45dp";
                    };
                    {
                      TextView;
                      textSize="13";
                      layout_marginTop="5dp";
                      textColor="0xFF000000";
                      text="分享软件";
                      id="shareButtonText";
                    };
                  };
                };
                {
                  LinearLayout;
                  layout_weight="1";
                  layout_height="match_parent";
                };
                {
                  LinearLayout;
                  gravity="center";
                  layout_height="match_parent";
                  {
                    LinearLayout;
                    orientation="vertical";
                    layout_height="match_parent";
                    gravity="center";
                    id="rewardButton";
                    {
                      ImageView;
                      layout_marginRight="10dp";
                      layout_width="45dp";
                      src="res/image/icons8-receive-cash.png";
                      layout_marginLeft="10dp";
                      layout_height="45dp";
                    };
                    {
                      TextView;
                      textSize="13";
                      layout_marginTop="5dp";
                      textColor="0xFF000000";
                      text="打赏捐赠";
                      id="rewardButtonText";
                    };
                  };
                };
              };
              {
                TextView;
                text="开发人员";
                textSize="23";
                layout_marginRight="15dp";
                layout_width="match_parent";
                layout_marginTop="5dp";
                textColor="0xFF000000";
                layout_marginLeft="15dp";
                id="developerText";
              };
              {
                LinearLayout;
                orientation="horizontal";
                layout_marginRight="15dp";
                layout_width="match_parent";
                layout_marginLeft="15dp";
                layout_height="110dp";
                {
                  CardView;
                  radius="10dp";
                  layout_marginRight="3dp";
                  layout_marginTop="5dp";
                  layout_marginLeft="5dp";
                  layout_weight="1";
                  layout_width="match_parent";
                  backgroundColor="#A5D6A7";
                  layout_marginBottom="5dp";
                  layout_height="match_parent";
                  {
                    LinearLayout;
                    orientation="vertical";
                    layout_height="match_parent";
                    layout_width="match_parent";
                    {
                      LinearLayout;
                      layout_marginRight="5dp";
                      layout_width="match_parent";
                      layout_marginTop="5dp";
                      layout_marginLeft="5dp";
                      gravity="left|center";
                      layout_height="50dp";
                      {
                        CardView;
                        radius="25dp";
                        elevation="0";
                        layout_width="50dp";
                        layout_height="50dp";
                        {
                          ImageView;
                          layout_width="match_parent";
                          src="res/image/head-xiaoyi.jpg";
                          layout_height="match_parent";
                        };
                      };
                      {
                        LinearLayout;
                        orientation="vertical";
                        layout_marginLeft="5dp";
                        layout_width="match_parent";
                        {
                          TextView;
                          text="依蚊";
                          textColor="0xFFFFFFFF";
                          textSize="20";
                          id="developerNameText1";
                        };
                        {
                          TextView;
                          text="Yiwen";
                          textColor="0xFF595959";
                          textSize="13";
                          id="developerEnNameText1";
                        };
                      };
                    };
                    {
                      TextView;
                      textSize="13";
                      layout_marginRight="7dp";
                      layout_marginTop="5dp";
                      layout_marginLeft="7dp";
                      id="developerLabelText1";
                      layout_width="match_parent";
                      layout_marginBottom="5dp";
                      textColor="0xFF000000";
                      layout_height="match_parent";
                      text="初闻不解曲中意，再听已是曲中人。";
                    };
                  };
                };
                {
                  CardView;
                  radius="10dp";
                  layout_marginRight="5dp";
                  layout_marginTop="5dp";
                  layout_marginLeft="3dp";
                  layout_weight="1";
                  layout_width="match_parent";
                  backgroundColor="#A5D6A7";
                  layout_marginBottom="5dp";
                  layout_height="match_parent";
                  {
                    LinearLayout;
                    orientation="vertical";
                    layout_height="match_parent";
                    layout_width="match_parent";
                    {
                      LinearLayout;
                      layout_marginRight="5dp";
                      layout_width="match_parent";
                      layout_marginTop="5dp";
                      layout_marginLeft="5dp";
                      gravity="left|center";
                      layout_height="50dp";
                      {
                        CardView;
                        radius="25dp";
                        elevation="0";
                        layout_width="50dp";
                        layout_height="50dp";
                        {
                          ImageView;
                          layout_width="match_parent";
                          src="res/image/head-laomao.jpg";
                          layout_height="match_parent";
                        };
                      };
                      {
                        LinearLayout;
                        orientation="vertical";
                        layout_marginLeft="5dp";
                        layout_width="match_parent";
                        {
                          TextView;
                          text="猫猫";
                          textColor="0xFFFFFFFF";
                          textSize="20";
                          id="developerNameText2";
                        };
                        {
                          TextView;
                          text="LaoMao MAG";
                          textColor="0xFF595959";
                          textSize="13";
                          id="developerEnNameText2";
                        };
                      };
                    };
                    {
                      TextView;
                      textSize="13";
                      layout_marginRight="7dp";
                      layout_marginTop="5dp";
                      layout_marginLeft="7dp";
                      id="developerLabelText2";
                      layout_width="match_parent";
                      layout_marginBottom="5dp";
                      textColor="0xFF000000";
                      layout_height="match_parent";
                      text="这不是一般的猫猫，这是王维诗里的猫猫∠( ᐛ 」∠)_";
                    };
                  };
                };
              };
              {
                LinearLayout;
                layout_width="match_parent";
                layout_height="200dp";
              };
              {
                TextView;
                textSize="13";
                text=[==[图标素材来源于icon8.com
部分资源来源于网络，如有侵权请联系作者进行更正]==];
                layout_width="match_parent";
                id="aboutBottomTipText";
                textColor="0xFF595959";
                gravity="center";
                layout_height="40dp";
              };
              {
                LinearLayout;
                layout_width="match_parent";
                layout_height="80dp";
              };
            };
            {
              LinearLayout;
              gravity="center";
              layout_width="match_parent";
              {
                CardView;
                radius="20dp";
                layout_marginBottom="5dp";
                layout_marginTop="5dp";
                alpha="20dp";
                CardBackgroundColor="0xFFFFFFFF";
                layout_width="100dp";
                layout_height="100dp";
                {
                  ImageView;
                  layout_width="match_parent";
                  src="res/image/logo1.png";
                  layout_height="match_parent";
                };
              };
            };
          };
        };
      };
    };
  };
};
